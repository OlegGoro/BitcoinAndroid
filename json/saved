import React, { Component } from 'react'
import {
    AppRegistry,
   Text,
   View,
} from 'react-native'

export default class AwesomeProject extends Component {


   constructor() {
      super()
      this.state = {}
   }

   test () {
     fetch("https://api.bitcoinaverage.com/ticker/global/USD/", {method: "GET"})
     .then((response) => response.json())
     .then((responseData) => {
this.setState({myText: responseData.bid})
     })
   }

   render() {
      return (
         <View>
            <Text>
               {this.state.myText}
               {this.test()}
            </Text>
         </View>
      );
   }
}


AppRegistry.registerComponent('AwesomeProject', () => AwesomeProject);
